\begin_layout Plain Layout
SYNOPSIS 
\end_layout

\begin_layout Plain Layout
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

boundary domain [bounding-box] [properties] 
\end_layout

\begin_layout Plain Layout

