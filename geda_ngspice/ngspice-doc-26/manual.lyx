\begin_layout Standard
\begin_inset Formula 
\begin{equation}
I(T)=I({\rm TNOM})\Bigl(1+TC_{1}(T-{\rm TNOM})+TC_{2}(T-{\rm TNOM})^{2}\Bigr)
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
or
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
V(T)=V({\rm TNOM})\Bigl(1+TC_{1}(T-{\rm TNOM})+TC_{2}(T-{\rm TNOM})^{2}\Bigr)
\end{equation}

\end_inset


\end_layout

