  <ident>=<value> <ident>=<value> ...
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout
Examples:
\end_layout

\begin_layout Plain Layout
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

